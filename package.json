{
  "name": "chess-simul-monorepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*",
    "supabase"
  ],
  "scripts": {
    "dev": "npm run dev -w web",
    "build": "node scripts/generate-env.mjs && npm run build -w web",
    "lint": "eslint apps packages --ext .ts,.tsx",
    "test": "npm run typecheck -w web",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "npm run typecheck -w web",
    "generate:env": "node scripts/generate-env.mjs",
    "supabase:start": "npm run start -w @chess-simul/supabase",
    "supabase:stop": "npm run stop -w @chess-simul/supabase",
    "supabase:reset": "npm run reset -w @chess-simul/supabase",
    "supabase:db:push": "npm run db:push -w @chess-simul/supabase",
    "supabase:gen:types": "npm run gen:types -w @chess-simul/supabase"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "eslint": "^9.15.0",
    "prettier": "^3.4.2",
    "typescript-eslint": "^8.18.0"
  }
}
