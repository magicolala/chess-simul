{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ng serve",
    "build": "ng build",
    "preview": "ng serve --configuration=production",
    "typecheck": "ng build --configuration=development --output-path ./dist/apps/web",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:a11y": "playwright test accessibility.spec.ts",
    "test:visual": "playwright test visual-regression.spec.ts",
    "test:visual:update": "playwright test visual-regression.spec.ts --update-snapshots"
  },
  "dependencies": {
    "@angular/build": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.6",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.0",
    "@angular/router": "^21.0.6",
    "@google/genai": "^1.34.0",
    "@supabase/supabase-js": "^2.89.0",
    "chess.js": "^1.4.0",
    "d3": "^7.9.0",
    "rxjs": "^7.8.2",
    "tailwindcss": "latest"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.57.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.0",
    "jsdom": "^26.0.0",
    "typescript": "~5.9.0",
    "vite": "^6.2.0",
    "vitest": "^3.0.6"
  }
}
